{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],

    "module": "ESNext",
    "moduleResolution": "Bundler",

    "types": ["bun-types"],
    "allowSyntheticDefaultImports": true,

    "strict": true,
    "skipLibCheck": true,

    "noEmit": true,

    "strictNullChecks": true
  },
  "include": ["src"]
}
